        + modalVideo({
            mp4: 'http://blob.bridgeplus.cn/video/mp4/zh/%E5%9B%A2%E9%98%9F.mov.mp4',
            title: 'Marksimos团队',
            className: 'marksimos-team'
        })(class="marksimos-team")
        + modalVideo({
            mp4: 'http://blob.bridgeplus.cn/video/mp4/zh/%E6%88%98%E7%95%A5.mp4',
            title: 'Marksimos战略',
            className: 'marksimos-strategy'
        })(class="marksimos-strategy")
        + modalVideo({
            mp4: 'http://blob.bridgeplus.cn/video/mp4/zh/%E5%81%9A%E5%86%B3%E7%AD%96.mp4',
            title: 'Marksimos做决策',
